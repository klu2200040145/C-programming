#include<stdio.h>
#include<math.h>
void NN(int a, int b, int c){
    float e = (-b + (sqrt(pow(b, 2)-4*a*c)))/(2*a);
    float d = (-b - (sqrt(pow(b, 2)-4*a*c)))/(2*a);
    printf("root1 : %f\n", e);
    printf("root2 : %f\n", d);
}
void main(){
    int a, b, c;
    printf("Enter a : ");
    scanf("%d", &a);
    printf("Enter b : ");
    scanf("%d", &b);
    printf("Enter c : ");
    scanf("%d", &c);
    NN(a, b, c);
}
